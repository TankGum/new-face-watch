<layout id="WatchFace" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="https://developer.garmin.com/downloads/connect-iq/resources.xsd">
  <bitmap id="heartIcon" filename="../images/heart.png"/>
  <bitmap id="stepIcon" filename="../images/step.png"/>
  <bitmap id="caloriesIcon" filename="../images/calories.png"/>
  <bitmap id="steepIcon" filename="../images/steep.png"/>
  <bitmap id="distanceIcon" filename="../images/distance.png"/>
  <bitmap id="ChargingStatus" filename="../images/electric.png"/>
  <label id="DeviceConnectionStatusDisplay" x="right" y="center" font="Gfx.FONT_LARGE" justification="Gfx.TEXT_JUSTIFY_CENTER" color="Gfx.COLOR_WHITE" />
  <bitmap id="connected" filename="../images/connected.png"/>
  <bitmap id="disConnect" filename="../images/disconnect.png"/>
  <drawable id="DrawLineLayout" class="DrawLineLayout"/>
  <drawable id="BatteryDisplay" class="BatteryDisplay"/>
  <drawable id="DateDisplay" class="DateDisplay"/>
  <drawable id="PhoneConnectionDisplay" class="PhoneConnectionDisplay" />
  <drawable id="DayNightDisplay" class="DayNightDisplay" />
  <drawable id="ChargingStatusDisplay" class="ChargingStatusDisplay" />
</layout>
